{
  "name": "nc/app2",
  "type": "project",
  "require": {
    "php": "^8.4",
    "nc/common": "*"
  },
  "scripts": {
    "app2": "php -S 0.0.0.0:8000 -t src"
  },
  "autoload": {
    "psr-4": {
      "Nc\\App2\\": "src/"
    }
  },
  "repositories": [
    {
